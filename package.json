{
  "name": "morpheus4j",
  "version": "2.2.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Morpheus is a migration tool for Neo4j. It aims to be a simple and intuitive way to migrate your database.",
  "license": "MIT",
  "author": {
    "name": "Mariano Zunino",
    "email": "marianoz@zoho.com",
    "url": "https://github.com/marianozunino"
  },
  "exports": {
    "./nest": "./dist/nest/index.js",
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "nest": [
        "dist/nest/index.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/marianozunino/morpheus"
  },
  "keywords": [
    "neo4j",
    "migrations",
    "migration tool",
    "nestjs",
    "cli"
  ],
  "bin": {
    "morpheus": "./dist/index.js"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@types/rimraf": "^3.0.2",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@nestjs/common": "^8.4.0",
    "commander": "^9.0.0",
    "crc": "^4.1.0",
    "cypher-query-builder": "^6.0.4",
    "joi": "^17.6.0",
    "neo4j-driver-core": "^4.4.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.5.4"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile",
    "transpile": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "lint": "eslint src/**/*.ts --fix",
    "morpheus": "node ./dist/index.js"
  }
}
